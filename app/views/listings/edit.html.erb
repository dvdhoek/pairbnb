<% provide(:title, "Edit user") %>
<h1>Update your listing</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@listing) do |f| %>


      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.submit "Save changes" %>
    <% end %>
  </div>
</div>